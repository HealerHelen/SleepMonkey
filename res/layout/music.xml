<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/aa"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <TableLayout
        android:id="@+id/tableLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="240dp" >


        <TableRow
            android:id="@+id/TableRow07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView11"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text=" " />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.丛林蛙鸣"
                android:textColor="#d1eeee"
                android:textSize="21dp" />

            <ImageButton
                android:id="@+id/ImageButton1"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginLeft="60dp"
                android:background="#00000000" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView06"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text=" " />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2.森林鸟鸣"
                android:textColor="#d1eeee"
                android:textSize="21dp" />

            <ImageButton
                android:id="@+id/ImageButton01"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_marginLeft="60dp"
                android:background="#00000000" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView07"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text=" " />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3.暖暖篝火"
                android:textColor="#d1eeee"
                android:textSize="21dp" />

            <ImageButton
                android:id="@+id/ImageButton04"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:layout_marginLeft="60dp"
                android:background="#00000000" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView08"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text=" " />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4.海风细浪"
                android:textColor="#d1eeee"
                android:textSize="21dp" />

            <ImageButton
                android:id="@+id/ImageButton07"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_marginLeft="60dp"
                android:background="#00000000" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView09"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text=" " />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5.四季清晨"
                android:textColor="#d1eeee"
                android:textSize="21dp" />

            <ImageButton
                android:id="@+id/ImageButton10"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:layout_marginLeft="60dp"
                android:background="#00000000" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView10"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text=" " />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="6.雨之涟漪"
                android:textColor="#d1eeee"
                android:textSize="21dp" />

            <ImageButton
                android:id="@+id/ImageButton13"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginLeft="60dp"
                android:background="#00000000" />
        </TableRow>
    </TableLayout>


    <TableRow
        android:id="@+id/TableRow06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp" >

        <ImageView
            android:id="@+id/hint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:src="@drawable/cs" />

        <TextView
            android:id="@+id/hint1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  聆听我们的声音❤"
            android:textColor="#ffffff"
            android:textSize="16sp" />
    </TableRow>

    <ImageButton
        android:id="@+id/ImageButton2"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_below="@+id/TableRow06"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:background="#00000000"
        android:src="@drawable/zt" />

    <ImageButton
        android:id="@+id/ImageButton3"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_alignBottom="@+id/ImageButton2"
        android:layout_marginLeft="22dp"
        android:layout_toRightOf="@+id/ImageButton2"
        android:background="#00000000"
        android:src="@drawable/tz" />

    <LinearLayout
        android:id="@+id/settime"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/button4"
        android:layout_below="@+id/button4"
        android:layout_toRightOf="@+id/TableRow06"
        android:orientation="vertical" >

        <ImageButton
            android:id="@+id/button1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:src="@drawable/sw" />
                <ImageButton
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:src="@drawable/ss" />
                        <ImageButton
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:src="@drawable/ls" />
    </LinearLayout>

    <Button
        android:id="@+id/button4"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/ImageButton3"
        android:text="设置定时" />

    <TextView
        android:id="@+id/show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/ImageButton2"
        android:text="计时15分钟"
        android:textColor="#ffffff" />

</RelativeLayout>